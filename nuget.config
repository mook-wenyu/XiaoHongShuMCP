<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- 本地 NuGet feed（用于开发调试） -->
    <add key="LocalFeed" value="D:\LocalNuGet" />

    <!-- GitHub Packages（用于 CI/CD 和团队协作）
         使用前需要配置 GitHub Token：
         dotnet nuget add source https://nuget.pkg.github.com/mook-wenyu/index.json -n github -u mook-wenyu -p YOUR_GITHUB_TOKEN
    -->
    <!-- <add key="github" value="https://nuget.pkg.github.com/mook-wenyu/index.json" /> -->

    <!-- 官方 NuGet 源 -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>

  <packageSourceMapping>
    <!-- HushOps.FingerprintBrowser 优先从本地 feed 和 GitHub Packages 获取 -->
    <packageSource key="LocalFeed">
      <package pattern="HushOps.FingerprintBrowser" />
    </packageSource>
    <!-- <packageSource key="github">
      <package pattern="HushOps.FingerprintBrowser" />
    </packageSource> -->

    <!-- 其他包从官方源获取 -->
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
