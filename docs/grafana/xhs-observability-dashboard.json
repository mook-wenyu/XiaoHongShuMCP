  "panels": [
    {
      "type": "row",
      "title": "UAM 概览",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
    },
    {
      "type": "stat",
      "title": "总响应",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
      "targets": [{
        "expr": "sum(uam_total_responses)",
        "legendFormat": "total"
      }]
    },
    {
      "type": "stat",
      "title": "2xx 成功",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1},
      "targets": [{
        "expr": "sum(uam_success_2xx)",
        "legendFormat": "2xx"
      }]
    },
    {
      "type": "stat",
      "title": "429/403",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
      "targets": [{"expr": "sum(uam_http_429)", "legendFormat": "429"}, {"expr": "sum(uam_http_403)", "legendFormat": "403"}]
    },
    {
      "type": "stat",
      "title": "Captcha Hints",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
      "targets": [{"expr": "sum(uam_captcha_hints)", "legendFormat": "hints"}]
    },
    {
      "type": "timeseries",
      "title": "端点响应耗时 p95",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 5},
      "targets": [{
        "expr": "histogram_quantile(0.95, sum(rate(uam_process_duration_ms_bucket[5m])) by (le, endpoint))",
        "legendFormat": "{{endpoint}}"
      }]
    },
    {
      "type": "timeseries",
      "title": "阶段耗时（Ensure/Input/Await/Aggregate/Scroll）",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 5},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(uam_stage_ensure_duration_ms_bucket[5m])) by (le, endpoint))", "legendFormat": "ensure {{endpoint}}"},
        {"expr": "histogram_quantile(0.95, sum(rate(uam_stage_input_duration_ms_bucket[5m])) by (le, endpoint))", "legendFormat": "input {{endpoint}}"},
        {"expr": "histogram_quantile(0.95, sum(rate(uam_stage_await_duration_ms_bucket[5m])) by (le, endpoint))", "legendFormat": "await {{endpoint}}"},
        {"expr": "histogram_quantile(0.95, sum(rate(uam_stage_aggregate_duration_ms_bucket[5m])) by (le, endpoint))", "legendFormat": "aggregate {{endpoint}}"},
        {"expr": "histogram_quantile(0.95, sum(rate(uam_stage_scroll_duration_ms_bucket[5m])) by (le, endpoint))", "legendFormat": "scroll {{endpoint}}"}
      ]
    },
    {
      "type": "row",
      "title": "定位/交互",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 13}
    },
    {
      "type": "timeseries",
      "title": "定位耗时 p95",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
      "targets": [{
        "expr": "histogram_quantile(0.95, sum(rate(locate_stage_duration_ms_bucket[5m])) by (le, role, name))",
        "legendFormat": "{{role}}/{{name}}"
      }]
    },
    {
      "type": "bargauge",
      "title": "定位尝试计数（5m）",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
      "targets": [{
        "expr": "sum(rate(locate_attempts_total[5m])) by (strategy, role, name)",
        "legendFormat": "{{strategy}} {{role}}/{{name}}"
      }]
    },
    {
      "type": "row",
      "title": "治理/异常",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 22}
