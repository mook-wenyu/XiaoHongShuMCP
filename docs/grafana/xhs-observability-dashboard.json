    {
      "type": "stat",
      "title": "UI 注入计数（应=0）",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 23},
      "targets": [{"expr": "sum(ui_injection_total)", "legendFormat": "total"}]
    },
    {
      "type": "timeseries",
      "title": "429/403 比例（5m）",
      "gridPos": {"h": 8, "w": 16, "x": 8, "y": 23},
      "targets": [
        {"expr": "sum(rate(uam_http_429[5m])) / sum(rate(uam_total_responses[5m]))", "legendFormat": "429 ratio"},
        {"expr": "sum(rate(uam_http_403[5m])) / sum(rate(uam_total_responses[5m]))", "legendFormat": "403 ratio"}
      ]
    }
  ],
